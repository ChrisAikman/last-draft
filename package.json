    "draft-js-image-plugin": "2.0.0-beta10",
    "draft-js-link-plugin": "1.1.0",
    "draft-js-linkify-plugin": "2.0.0-beta10",
    "draft-js-mention-plugin": "2.0.0-beta10",
    "draft-js-plugins-editor": "2.0.0-beta10",
    "draft-js-resizeable-plugin": "2.0.0-beta10",
    "immutable": "~3.7.4",
    "last-draft-js-sidebar-plugin": "1.1.0",
    "last-draft-js-toolbar-plugin": "1.1.0"
  },
  "keywords": [
    "ld",
